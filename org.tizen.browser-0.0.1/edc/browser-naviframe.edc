         part { name: "optionheader.bottom.padding";
            type: RECT;
            scale: 1;
            clip_to: "optionheader_clip";
            description { state: "default" 0.0;
               min: 0 NAVIFRAME_OH_PADDING_INC;
               rel1 { relative: 0.0 1.0; to: "optionheader2"; }
               rel2 { relative: 1.0 1.0; to: "optionheader2"; }

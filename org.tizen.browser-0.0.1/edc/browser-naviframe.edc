               rel1 { relative: 0.0 1.0; to: "optionheader"; }
               rel2 { relative: 1.0 1.0; to: "optionheader"; }
               fixed: 0 1;
               align: 0.0 0.0;
               visible: 0;
            }
         }
         part { name: "optionheader2";
            type: SWALLOW;
            scale: 1;
            clip_to: "optionheader_clip";
